{
  "name": "nyxy-portfolio",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "host": "vite --host",
    "sass-dev": "sass --watch --update --style=expanded src/assets/sass:src/assets/css",
    "sass-prod": "sass --no-source-map --style=compressed src/assets/sass:src/assets/css",
    "postcss:watch": "postcss src/assets/css/main.css --dir public --watch",
    "postsass": "concurrently \"npm run sass-dev\" \"npm run postcss:watch\""
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-multi-carousel": "^2.8.3"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.0.1",
    "postcss": "^8.4.21",
    "postcss-sort-media-queries": "^4.3.0",
    "sass": "^1.60.0",
    "vite": "^4.2.0"
  },
  "browserslist": [
    "> 0.2%",
    "last 5 versions",
    "Android >= 4",
    "Firefox >= 20",
    "iOS >= 8"
  ]
}